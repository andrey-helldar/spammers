{
    "name": "andrey-helldar/spammers",
    "description": "Simple package to prevent accessing of spammers to Laravel application",
    "keywords": ["spammers", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/spammers/issues",
        "source": "https://github.com/andrey-helldar/spammers"
    },
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/console": "^5.4|^6.0",
        "illuminate/database": "^5.4|^6.0",
        "illuminate/http": "^5.4|^6.0",
        "illuminate/support": "^5.4|^6.0",
        "illuminate/validation": "^5.4|^6.0",
        "jaybizzle/laravel-crawler-detect": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*|1.*",
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Spammers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Helldar\\Spammers\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Spammers\\ServiceProvider"
            ]
        }
    }
}
