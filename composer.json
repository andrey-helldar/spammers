{
  "name": "andrey-helldar/spammers",
  "description": "Simple package to prevent accessing of spammers to Laravel application",
  "keywords": [
    "spammers",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Helldar",
      "email": "helldar@ai-rus.com"
    }
  ],
  "support": {
    "issues": "https://github.com/andrey-helldar/spammers/issues",
    "source": "https://github.com/andrey-helldar/spammers"
  },
  "require": {
    "php": ">=5.6.4",
    "illuminate/console": "~5.4",
    "illuminate/database": "~5.4",
    "illuminate/http": "~5.4",
    "illuminate/support": "~5.4",
    "illuminate/validation": "~5.4"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Helldar\\Spammers\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Helldar\\Spammers\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Helldar\\Spammers\\ServiceProvider"
      ]
    }
  }
}
